{
	"name": "stefna/secrets-manager-json-provider",
	"description": "Secrets manager for stefna projects",
	"type": "library",
	"license": "proprietary",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"authors": [
		{
			"name": "Andreas Sundqvist",
			"email": "andreas@stefna.is"
		}
	],
	"require": {
		"php": "^7.1 || ^8.0",
		"ext-json": "*"
	},
	"require-dev": {
		"stefna/codestyle": "^1.5",
		"phpstan/phpstan": "^0.12.62"
	},
	"autoload": {
		"psr-4": {
			"Stefna\\SecretsManager\\Provider\\JsonProvider\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Stefna\\SecretsManager\\Provider\\JsonProvider\\Tests\\": "tests/"
		}
	},
	"repositories": [
		{"type": "composer", "url": "https://repo.packagist.com/stefnadev/"}
	]
}
